{
  "version": 1,
  "paths": {
    "/admin/users": {
      "name": "/admin/users",
      "lambdaFunction": "hydronodeAdminUserManager",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    },
    "/users/{username}": {
      "name": "/users/{username}",
      "lambdaFunction": "hydronodeAdminUserManager",
      "permissions": {
        "setting": "private",
        "auth": [
          "create"
        ]
      }
    }
  }
}